{
    "name": "emag-tech-labs/messenger-mongo-bundle",
    "description": "A Mongo transport for the Symfony Messenger component",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Iosif Chiriluta",
            "email": "iosifpub@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "symfony/messenger": "^4.0||^5.0",
        "doctrine/mongodb-odm-bundle": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "symfony/serializer": "^5.0",
        "symfony/property-access": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "EmagTechLabs\\MessengerMongoBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EmagTechLabs\\MessengerMongoBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "ext-mongo": "1.16.1"
        }
    }
}
